        <comment>Add data for available languages on cv's 4th page</comment>

        <createTable tableName="languages">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints  primaryKey="true" nullable="false"/>
            </column>
            <column name="language_name" type="varchar(20)">
                <constraints unique="true" nullable="false"/>
            </column>
            <column name="is_top" type="boolean">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addDefaultValue tableName="languages"
                     columnName="is_top"
                     defaultValueBoolean="false"/>

